@model Category


<div class="container-fluid px-4">
    <h2 class="mt-4 text-center text-uppercase" style="letter-spacing:4px">Are you sure want to delete this category</h2>
</div>

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
      
        <ul class="list-group">
            <li class="list-group-item">Category Id : @Model.category_id</li>
            <li class="list-group-item">Category Name : @Model.category_name</li>
           

        </ul>
        <a href="@Url.Action("deleteCategory","Admin",new{id=Model.category_id})" class="btn btn-danger">Delete</a>
        <a href="@Url.Action("fetchGategory","Admin")" class="btn btn-warning">Back to List</a>

    </div>

    <div class="col-md-2"></div>




</div>